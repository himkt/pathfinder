{
  "mcpServers": {
    "pathfinder-python": {
      "command": "pathfinder",
      "args": [
        "-w", ".",
        "-e", "py",
        "-e", "pyi",
        "-s", "uv", "run", "pyright-langserver", "--", "--stdio"
      ],
      "env": {
        "LOG_LEVEL": "debug"
      }
    },
    "pathfinder-rust": {
      "command": "pathfinder",
      "args": [
        "-w", ".",
        "-e", "rs",
        "-s", "rust-analyzer"
      ],
      "env": {
        "LOG_LEVEL": "debug"
      }
    }
  }
}
